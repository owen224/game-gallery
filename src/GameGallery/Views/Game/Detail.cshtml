@{
    Layout = null;
}


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
            <h1>@ViewBag.SeriesTitle</h1>
            <h2>Ranking #@ViewBag.Ranking</h2>

            <h5>Description:</h5>
            <div>@Html.Raw(ViewBag.Description)</div>


            @if (ViewBag.Creaters.Length > 0)
            {
            <h5>Creaters:</h5>
            <div>   
                  <ul>
                    @foreach (var creater in ViewBag.Creaters)
                    {
                        <li>@creater</li>
                    }
                   </ul>
            </div>
            }
    </div>
</body>
</html>
